<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title><%= data.page.title || "Full-time, Freelance and Part-time Jobs in Singapore" %></title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width">
  <link rel="stylesheet" href="assets/sass/main.css">
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600' rel='stylesheet' type='text/css'>
  <script src="assets/js/vendor/modernizr-2.6.2.min.js"></script>
</head>
<body class="<%= page_classes %>">

  <header class="branding mast clearfix">
    <div class="container">
      <a href="/">Hireme</a>
      <ul class="user-list align-right">
        <li><a href="">Register</a></li>
        <li><a href="">Sign in</a></li>
        <li><a href="/post.html" class="zocial primary">Post a Job</a></li>
      </ul>
    </div>
  </header>


  <section class="filters clearfix">
    <div class="container">

      <ul class="job-type">
        <li><a href="#">Fulltime</a></li>
        <li><a href="">Part-time</a></li>
        <li><a href="">Freelance</a></li>
        <li><a href="">Internship</a></li>
      </ul>

      <ul class="job-category align-right">
        <li>
          <label for="">Showing</label>
          <select name="" id="">
            <option>All jobs</option>
            <option>Front-end Development</option>
            <option>Back-end Development</option>
            <option>Mobile (iPhone, Android, other)</option>
            <option>Interaction Design</option>
            <option>Graphics Design</option>
            <option>Content Strategy</option>
            <option>Information Architecture</option>
            <option>Customer Support</option>
            <option>Management</option>
            <option>Accounting</option>
          </select>
        </li>
        <li>
          <form action="">
            <input type="text" name="" id="" placeholder="Search">
          </form>
        </li>
      </ul>
    </div>
  </section>


  <%= yield %>



  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="assets/js/vendor/jquery-1.8.3.min.js"><\/script>')</script>
  <script src="assets/js/jquery.wookmark.min.js"></script>
  <script src="assets/js/plugins.js"></script>
  <script src="assets/js/main.js"></script>

</body>
</html>